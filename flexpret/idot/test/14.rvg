[mu []
[[file {Reset_Handler} {
  Reset_Handler:
    [id init]
    [[single-else get-coreid] a3
      {
        [[single-else get-hartid] t5 {}
          {
            [def (others) [memregion t0 t1 noc-base-address words 2 to 16]]
            [def (scattering) [cyclify a0 a1 others times 5]]
            [[nonzero-team pariterate-at-rate] {address} of scattering doing 2 per 19 cycles
              {
                rdcycle t3
                sw t3 [id address]
              }]
          }]
      }
      {
        [using-noc a3 [mu []
          {
            [single-hart t1
              {
                [delay a4 32]
                [iterate-at-rate t2 of [.. t4 0 to 5] doing 1 per 19 cycles
                  {
                    [noc-read t0 0]
                    [sim-print t0 t3]
                  }]
              }]
          }]]
      }]
    [single-hart t0
      {
        rdcycle t3
        [sim-print t3 t4]
      }]
    [id die]
  }]]
]
