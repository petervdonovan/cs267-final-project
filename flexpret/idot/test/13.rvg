[mu []
[[file {Reset_Handler} {
  Reset_Handler:
    [id init]
    [[single-else get-coreid] t5
      {
        [iterate {address} of [memregion t0 t1 noc-base-address words 2 to 16]
          {
            li t2 42
            sw t2 [id address]
          }]
      }
      {
        [using-noc a3 [mu []
          {
            [delay a4 32]
            [single-hart t1
              {
                [noc-read t0 0]
                [sim-print t0 t3]
              }]
          }]]
      }]
    [id die]
  }]]
]
