[mu []
[def (print-hartid)
  [forkjoin-harts t0 [lam [(n)]
      [critical-harts t1 t4
      {
        [sim-print-int n t2 t3]
      }]]]]
[def (cycles-of) [cycles? print-hartid]]
[[file {Reset_Handler} {
  Reset_Handler:
    [id init]
    [single t2 [sim-print-int cycles-of t0 t1]]
    rdcycle t5
    [id print-hartid]
    rdcycle t6
    sub t5 t6 t5
    [critical-harts t0 t2
      [sim-print t5 t1]]
    [id die]}]]
]
